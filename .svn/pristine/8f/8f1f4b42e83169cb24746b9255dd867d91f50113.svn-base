			//获取详情，明细
			getDefaultAnddetails(id){

				//获取采购订单详情上方
				this.axios.get('/receipt/get/'+id+'?uid='+this.uid).then((res)=>{

					if(res.data.status==200){
						let data=res.data.rows
						// this.type=data.type

						this.address=data.address//发货人地址
						this.deliverName=data.deliverName//收货方名称
						this.deliverAddress=data.deliverAddress
						this.formValidate.deliverTel=data.deliverTel//收货人电话
						this.formValidate.deliverMobile=data.deliverMobile//收货人手机
            			
            			//仓库
	            		this.formValidate.warehouseName=data.deliverWarehouseName//仓库名
	            		this.warehouseCode=data.deliverWarehouseCode//仓库编号
	            		this.warehouseId=data.deliverWarehouseId//仓库名id

	            		this.formValidate.supplierName=data.supplierName//供应商名称
		            	this.supplierId=data.supplierId//供应商id
						this.recordNo=data.recordNo//单号
						// totalQty:this.totalQty,//采购数量
						this.deliveryQty=data.deliveryQty//已发运数量
						this.logistics=data.logistics//物流公司
						this.logisticsNo=data.logisticsNo//物流编号
						
						this.poDate=data.poDate//订单日期
						this.recordNo=data.recordNo
						this.totalAmount=data.totalAmount
						this.totalCount=data.totalCount
						if(data.deliveryDate==undefined){
							this.deliveryDate=''
						}else{
							this.deliveryDate=data.deliveryDate
						}
						this.addtx = res.data.rows.recordNo
						this.status=res.data.rows.status
						this.upData=res.data.rows.fileList
						this.upData1=res.data.rows.fileList
						if(res.data.rows.status==88){
							res.data.rows.statusDesc='已完成'
						}
						this.cgxqInfo = res.data.rows
						this.recordNo = res.data.rows.recordNo
						if(data.type==1){
							this.modeldztype='进货单'
						}
						if(data.type==2){
							this.modeldztype='退货单'
						}
						//修改附件的链接
						let strvalue=[]//id链接
						if(this.upData==null){//判断是否有附件
							this.upData=[]
						}else{
							for(let i =0;i<this.upData.length;i++){
								if(this.upData[i].thumbId==''){
									this.upData.splice(i,1)
								}else{
									strvalue.push(this.upData[i].thumbId)
								}
							}
							let s1 = strvalue.join(',')
							this.attridMore = s1//id链接后字符串

						}
						this.cgxqInfo.createTime=new Date(this.cgxqInfo.createTime).toLocaleDateString().replace(/\//g,'-')
						this.cgxqInfo.deliveryDate=new Date(this.cgxqInfo.deliveryDate).toLocaleDateString().replace(/\//g,'-')
						// if(this.cgxqInfo.recordDate=='Invalid Date'){
						// 		this.cgxqInfo.recordDate='未指定'
						// 	}
						// 	if(this.cgxqInfo.deliveryDate=='Invalid Date'){
						// 		this.cgxqInfo.deliveryDate='未指定'
						// 	}

					if(res.data.rows.status==1){
						this.editshow=false
						this.sh=true
						this.qs=true
						this.zf=true
						this.dy=false
						this.wc=false
						this.xiugaishow=true
						this.shouhuoshow=false
						this.shouhuoshowsave=false
						this.isdelisshow=true
						this.activeSend=1
						this.isSuccess=false
					}
					//草稿
					if(res.data.rows.status==0){
						this.editshow=false
						this.sh=false
						this.qs=false
						this.zf=false
						this.dy=false
						this.wc=false
						this.xiugaishow=false
						this.isdelisshow=true
						this.activeSend=1
						this.isSuccess=false
					}
					//已审核
					if(res.data.rows.status==15){
						this.zf=true
						this.editshow=false
						this.dy=false
						this.qs=false
						this.sh=false
						this.wc=true
						this.shouhuoshow=false
						this.shouhuoshowsave=false
						this.xiugaishow=false
						this.isdelisshow=true
						this.activeSend=2
						this.isSuccess=false
					}
					
					
					if(res.data.rows.status==77){
						this.editshow=false
						this.sh=false
						this.qs=false
						this.zf=true
						this.dy=false
						this.wc=false
						this.xiugaishow=false
						this.isdelisshow=true
						this.isSuccess=false
					}
					//已作废
					if(res.data.rows.status==19){
						this.zf=false
						this.editshow=false
						this.dy=false
						this.qs=false
						this.sh=false
						this.wc=false
						this.shouhuoshow=false
						this.shouhuoshowsave=false
						this.xiugaishow=false
						this.isdelisshow=true
						this.isSuccess=false
					}
					//完成
					if(res.data.rows.status==88){
						this.xiugaishow=false
						this.zf=false
						this.editshow=false
						this.dy=true
						this.qs=false
						this.wc=false
						this.sh=false
						this.shouhuoshowsave=false
						this.shouhuoshow=false
						this.isdelisshow=false
						this.activeSend=3
						this.isSuccess=true
					}

					}
				})
				//获取采购订单明细下方
				this.axios.get('/receipt/detail/'+id+'?uid='+this.uid).then((res)=>{
					if(res.data.status==200){
						if(res.data.rows.length==0){
							this.cgListInfo=[{skuName:this.$t('operation.yhqHome.choosesku'),allprice:0,qty:0,costPrice:0}]//'选择商品'
							this.cunhuodata=[{skuName:this.$t('operation.yhqHome.choosesku'),allprice:0,qty:0,costPrice:0}]//'选择商品'
						}else{
							this.cgListInfo = res.data.rows
							this.mxtotal=res.data.total
							this.mxpageSize=res.data.pageSize
							let totalP;//添加总价变量
							this.cgListInfo.forEach((item)=>{
								totalP = item.costPrice*item.qty
								item['totalP'] = totalP//添加一个属性总价
								
							})
							this.cunhuodata=res.data.rows//收货商品明细
							this.cunhuodata.forEach((x)=>{
								if(x.qty==null){
									return
								}else{
									x.allprice=x.qty*x.costPrice
									x.poDetailId=x.id
								}

							})
						}
						
					}
				})
			},
