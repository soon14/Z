					if(res.data.status==200){
						this.getSystem=res.data.rows
						//只有一个权限不能点击展开
						if(this.getSystem.length==1){
							this.listisshow=false
							this.imgShow = false
						}
					}else{
						this.$notify({
					        title: "",//'错误',
					        message: res.data.errorMessage,
					        type: 'error',
					        position: 'bottom-right'
				        });
					}
				})
