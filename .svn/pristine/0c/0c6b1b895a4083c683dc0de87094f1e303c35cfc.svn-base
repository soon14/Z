                        this.$store.commit(types.LOGIN, data.rows.uid);//登陆凭证uid
                        this.$store.commit(types.USER, data.rows);//个人信息
                        this.lockMainisshow=false//锁屏界面隐藏
                        this.$router.push('/lock')//登陆成功跳转过度页面
