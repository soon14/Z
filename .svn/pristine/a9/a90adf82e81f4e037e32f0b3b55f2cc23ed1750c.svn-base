                					arr.push(x.label)
                					this.infopay=arr.join(',')
                				}
                			})
                		})
						//停用启用显示
						if(data.status==1){
							this.isstart=false
							this.isstop=true
							this.issdel=true
						}
						if(data.status==99){
							this.isstart=true
